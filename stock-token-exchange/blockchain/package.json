{
  "name": "stock-token-exchange",
  "version": "1.0.0",
  "description": "Stock Token Exchange with Cartesi for off-chain computation",
  "main": "index.js",
  "dependencies": {
    "@cartesi/compute-sdk": "1.3.0",
    "@openzeppelin/contracts": "^4.9.6",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "axios": "^1.6.7",
    "chai": "^4.3.10",
    "dotenv": "^16.4.5",
    "ethers": "^6.14.3",
    "hardhat": "^2.19.5",
    "hardhat-deploy": "^0.12.4",
    "hardhat-gas-reporter": "1.0.8",
    "solidity-coverage": "^0.8.6",
    "strip-bom": "^4.0.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "prevalidate": "node validate-packages.js --verbose",
    "prebuild": "node prebuild-validate.js && node strip-boms.js",
    "build": "hardhat compile",
    "deploy": "hardhat deploy --network localhost",
    "console": "hardhat console --network localhost",
    "test": "hardhat test",
    "build-machine": "cd ../cartesi-machine && ./build-machine.sh",
    "export:frontend": "ts-node scripts/export-deployments.ts",
    "generate:deployments": "node ../frontend/scripts/generate-deployments.js",
    "validate:packages": "node validate-packages.js",
    "validate:packages:fix": "node validate-packages.js --fix",
    "validate:packages:verbose": "node validate-packages.js --verbose",
    "validate:prebuild": "node prebuild-validate.js",
    "validate:prebuild:verbose": "node prebuild-validate.js --verbose",
    "clean:corrupted": "node validate-packages.js --fix --verbose && npm run build",
    "build:safe": "node prebuild-validate.js --verbose && npm run build",
    "clean:install": "rm -rf node_modules package-lock.json && npm install --legacy-peer-deps",
    "fix:versions": "node fix-versions.js",
    "install:safe": "node fix-versions.js && npm run validate:packages",
    "test:version-fix": "bash test-version-fix.sh"
  },
  "keywords": [
    "cartesi",
    "blockchain",
    "dex",
    "exchange",
    "stock-token"
  ],
  "author": "",
  "license": "MIT",
  "overrides": {
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "ethers": "^6.14.3",
    "hardhat-gas-reporter": "1.0.8",
    "@typechain/ethers-v6": "^0.5.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "resolutions": {
    "**/ethers": "^6.14.3",
    "**/@nomicfoundation/hardhat-ethers": "^3.0.8"
  }
}
