{
  "name": "stock-token-exchange",
  "version": "1.0.0",
  "description": "Stock Token Exchange with Cartesi for off-chain computation",
  "main": "index.js",
  "dependencies": {
    "@cartesi/compute-sdk": "1.3.0",
    "@openzeppelin/contracts": "^4.8.2",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.8",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.19",
    "axios": "^1.6.7",
    "chai": "^4.3.10",
    "dotenv": "^16.4.5",
    "ethers": "^6.14.3",
    "hardhat": "^2.19.5",
    "hardhat-deploy": "^0.12.4",
    "hardhat-gas-reporter": "^1.0.10",
    "solidity-coverage": "^0.8.6",
    "strip-bom": "^4.0.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "precompile": "node scripts/ensure-compiler.js",
    "prebuild": "node scripts/prebuild-validate.js",
    "build": "npm run precompile && hardhat compile",
    "build:safe": "npm run prebuild && npm run build",
    "build:force": "npm run precompile && hardhat compile --force",
    "build:clean": "hardhat clean && npm run build",
    "compile": "npm run build",
    "deploy": "hardhat deploy --network localhost",
    "console": "hardhat console --network localhost",
    "test": "npm run build && hardhat test",
    "test:safe": "npm run build:safe && hardhat test",
    "build-machine": "cd cartesi-machine && ./build-machine.sh",
    "export:frontend": "ts-node scripts/export-deployments.ts",
    "generate:deployments": "node frontend/scripts/generate-deployments.js",
    "validate:dependencies": "node scripts/validate-dependencies.js",
    "fix:dependencies": "node scripts/fix-dependencies.js",
    "install:safe": "npm run validate:dependencies && npm install --legacy-peer-deps",
    "clean:install": "rm -rf node_modules package-lock.json && npm run install:safe",
    "health:check": "node scripts/dependency-health-check.js",
    "compiler:check": "node scripts/ensure-compiler.js",
    "compiler:clean": "rm -rf cache artifacts ~/.cache/hardhat-nodejs",
    "compiler:recover": "node scripts/runtime-compiler-recovery.js",
    "compiler:recover:force": "node scripts/runtime-compiler-recovery.js --force --verbose",
    "docker:build": "npm run build:safe",
    "strip:boms": "node strip-boms.js"
  },
  "keywords": [
    "cartesi",
    "blockchain",
    "dex",
    "exchange",
    "stock-token"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "ethers": "^6.14.3",
    "hardhat-gas-reporter": "^1.0.10",
    "@typechain/ethers-v6": "^0.5.1"
  },
  "peerDependencies": {
    "ethers": "^6.14.3",
    "hardhat": "^2.19.5"
  },
  "resolutions": {
    "**/ethers": "^6.14.3",
    "**/@nomicfoundation/hardhat-ethers": "^3.0.8"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
